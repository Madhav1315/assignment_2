import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int compare = 0;
        while (true) {
            int user = input.nextInt();
            compare += user;
            if (compare < 0) {
                break;
            } else {
                System.out.println(user);
            }
        }
        input.close();
    }
}
